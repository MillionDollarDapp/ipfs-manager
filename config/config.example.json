{
  "env": "dev",
  "uploadDir": "/home/ubuntu/uploads",
  "expireAfterSeconds": 43200,

  "aws": {
    "dev": {
      "region": "local",
      "endpoint": "http://localhost:8000"
    },
    "live": {
      "region": "eu-central-1"
    }
  },

  "dynamoDb": {
    "table_files": "Files",
    "table_variables": "Variables",
    "all": {
      "apiVersion": "2012-10-08"
    }
  },

  "s3": {
    "bucket": "bucket-name",
    "all": {
      "apiVersion": "2006-03-01"
    },
    "dev": {
      "endpoint": "http://localhost:4567"
    }
  },

  "web3": {
    "network": "5777",

    "networks": {
      "5777": {
        "provider": "ws://localhost:8545",
        "dappGenesis": 0
      },
      "1": {
        "provider": "wss://mainnet.infura.io/ws",
        "dappGenesis": 0
      },
      "4": {
        "provider": "wss://rinkeby.infura.io/ws",
        "dappGenesis": 2921676
      }
    }
  }
}